<%= render partial: 'shared/form_errors', locals: { form: destination } %>

<%= form_for destination do |f| %>
  <%= f.text_field :city, placeholder: 'city' %>
  <%= f.text_field :country, placeholder: 'country' %>
  <%= f.text_field :image_url, placeholder: 'image url' %>
  <%= f.text_field :currency, placeholder: 'currency' %>
  <%= f.text_field :language, placeholder: 'language' %>
  <%= f.submit %>
<% end %>
